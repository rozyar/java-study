package br.com.fiap.motors.model;

public class View {
	public static void main(String[] args) {
		//Instanciar o Carro
		Carro fusca = new Carro();
		//Colocar informacoes nos atributos
		fusca.modelo = "fusca";
		fusca.ano = 1999;
		fusca.ano = 2;
		//Calcular a idade do carro
		int idade = fusca.calcularIdade(2023);
		//Exibir a idade
		System.out.println("A idade do " + fusca.modelo + " Ã© " + idade + " anos.");
	}
}
